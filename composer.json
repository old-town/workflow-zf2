{
  "name": "old-town/workflow-zf2",
  "description": "Integration Workflow and ZF2",
  "type": "library",
  "keywords": [
    "zf2",
    "workflow",
    "zf2 module"
  ],
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Malofeykin Andrey",
      "email": "and-rey2@yandex.ru"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.5.0",
    "old-town/workflow": "^0.1",
    "zendframework/zend-mvc": "~2.4.0",
    "zendframework/zend-view": "~2.4.0",
    "zendframework/zend-stdlib": "~2.4.0",
    "zendframework/zend-servicemanager": "~2.4.0",
    "zendframework/zend-modulemanager": "~2.4.0"
  },
  "require-dev": {
    "fabpot/php-cs-fixer": "~1.11.0",
    "phpunit/phpunit": "~4.8.0",
    "phpunit/phpcov": "~2.0.0",
    "zendframework/zend-loader": "~2.4.0",
    "zendframework/zend-test": "~2.4.0",
    "zendframework/zend-serializer": "~2.4.0",
    "zendframework/zend-log": "~2.4.0",
    "zendframework/zend-i18n": "~2.4.0",
    "zendframework/zend-json": "~2.4.0",
    "zendframework/zend-config": "~2.4.0",
    "zendframework/zend-version": "~2.4.0",
    "satooshi/php-coveralls": "~1.0.0"
  },
  "autoload": {
    "psr-4": {
      "OldTown\\Workflow\\ZF2\\": "src/"
    },
    "classmap": [
      "./Module.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "OldTown\\Workflow\\ZF2\\PhpUnit\\Test\\": "test/phpunit/tests",
      "OldTown\\Workflow\\ZF2\\PhpUnit\\TestData\\": "test/phpunit/_files"
    },
    "classmap": [
      "./test/phpunit/Bootstrap.php"
    ]
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:old-town/old-town-workflow.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:old-town/old-town-propertyset.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:old-town/old-town-log.git"
    }
  ]
}
